{
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "syntax-dynamic-import",
      "transform-class-properties",
      "react-hot-loader/babel",
      "transform-object-rest-spread",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "semantic-ui-react"
          ]
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@modules": "./node_modules",
            "@root": "./src/",
            "@components": "./src/components",
            "@layouts": "./src/layouts",
            "@views": "./src/views",
            "@styles": "./src/styles/",
            "@data": "./data/"
          }
        }
      ]
    ]
  }